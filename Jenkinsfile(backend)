pipeline{
    agent any
        stages{
            stage('git pull'){
                steps{
                git url: 'https://github.com/sravya-nanduri/nodejs-new.git', branch:'master'
                }
            }
            stage('building the app'){
             steps{
                sh '''
                sudo su -
                npm install
                nohup npm start > output.log 2>&1 &
                ps aux | grep node
                '''
             }

            }
        }
}
